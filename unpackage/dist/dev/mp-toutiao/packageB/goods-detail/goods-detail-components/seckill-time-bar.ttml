<view class="timer-show data-v-5b356db2" style="{{'background:'+(configInfo.web_site_color)+';'}}"><view class="content-wrap data-v-5b356db2"><view class="goods-info-block data-v-5b356db2"><view class="info-goods data-v-5b356db2"><view class="price-block data-v-5b356db2"><view class="price data-v-5b356db2">¥<text class="data-v-5b356db2">{{goodsInfo.price}}</text></view><view class="unit data-v-5b356db2">{{"/"+goodsInfo.unit}}</view></view><view class="sales-volume data-v-5b356db2"><text hidden="{{!(goodsInfo.monthNum>=1)}}" class="data-v-5b356db2">{{'月销售量'+goodsInfo.monthNum+goodsInfo.unit+' ,'}}</text><text hidden="{{!(goodsInfo.count>=1)}}" class="data-v-5b356db2">{{"库存"+goodsInfo.count+goodsInfo.unit}}</text></view></view><view class="timer-block data-v-5b356db2"><view class="text data-v-5b356db2">{{calcTipTitle+":"}}</view><block tt:if="{{endTime.hours&&endTime.minutes&&endTime.second}}"><view class="tim-group-block data-v-5b356db2"><view class="tim-block hour data-v-5b356db2" style="{{'color:'+(configInfo.web_site_color)+';'}}">{{''+endTime.hours+''}}</view><view class="span-1 data-v-5b356db2">:</view><view class="tim-block min data-v-5b356db2" style="{{'color:'+(configInfo.web_site_color)+';'}}">{{''+endTime.minutes+''}}</view><view class="span-1 data-v-5b356db2">:</view><view class="tim-block second data-v-5b356db2" style="{{'color:'+(configInfo.web_site_color)+';'}}">{{''+endTime.second+''}}</view></view></block></view></view></view></view>