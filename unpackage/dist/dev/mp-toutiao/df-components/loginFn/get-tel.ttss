.grey1-color {
  color: #374050;
}
.grey2-color {
  color: #91939c;
}
.login-tips-block {
  position: fixed;
  z-index: 980;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background: rgba(0, 0, 0, 0.3);
}
.login-tips-block .login-tips {
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  margin: auto;
  width: 600rpx;
  height: 600rpx;
  background: #ffffff;
}
.login-tips-block .login-tips .icon-top-play image {
  position: absolute;
  left: 50%;
  -webkit-transform: translateX(-50%) translateY(-50%);
          transform: translateX(-50%) translateY(-50%);
  width: 280rpx;
  height: 280rpx;
}
.login-tips-block .login-tips .h4 {
  margin-top: 175rpx;
  text-align: center;
  font-weight: 600;
  color: #374050;
  font-size: 36rpx;
}
.login-tips-block .login-tips .h5 {
  margin-top: 40rpx;
  color: #374050;
  font-size: 30rpx;
  text-align: center;
}
.login-tips-block .login-tips .h5 text {
  display: block;
  line-height: 50rpx;
}
.login-tips-block .login-tips .say-no,
.login-tips-block .login-tips .login-btn {
  display: flex;
  justify-content: center;
  align-items: center;
  margin: 35rpx auto 0;
  width: 536rpx;
  height: 88rpx;
  color: #ffffff;
  font-size: 30rpx;
}
.login-tips-block .login-tips .say-no {
  margin-top: 10rpx;
  background: transparent;
  text-decoration: underline;
}
