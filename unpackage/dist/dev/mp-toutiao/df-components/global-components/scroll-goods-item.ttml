<view data-event-opts="{{[['tap',[['navigateToFn',[['o',['url','/packageB/goods-detail/goods-detail?goods_id='+goodsId]]]]]]]}}" class="goods-scroll-item data-v-3d85696f" bindtap="__e"><view class="image-wrap data-v-3d85696f"><img-bg-css vue-id="7326d956-1" src="{{goodsImage}}" class="data-v-3d85696f" bind:__l="__l"></img-bg-css><block tt:if="{{isShowSalesSumBlock}}"><view class="icon-hot-wrap data-v-3d85696f"><icon-hot-wrap vue-id="7326d956-2" text="{{'已抢'+calcSalesSum+'件'}}" class="data-v-3d85696f" bind:__l="__l"></icon-hot-wrap></view></block></view><block tt:if="{{tagPath}}"><view class="tag-img-wrap data-v-3d85696f"><image class="image-dom data-v-3d85696f" src="{{tagImgSrc}}"></image></view></block><view class="goods-name-block data-v-3d85696f"><block tt:if="{{isNew}}"><image class="icon-new data-v-3d85696f" src="{{$root.m0}}"></image></block><block tt:if="{{isHot}}"><image class="icon-hot data-v-3d85696f" src="{{$root.m1}}"></image></block>{{''+goodsName+''}}</view><view class="goods-tag-block data-v-3d85696f"><block tt:if="{{$root.m2}}"><view class="goods-tag data-v-3d85696f">{{goodsTag}}</view></block></view><view class="price-block data-v-3d85696f"><view class="price-icon data-v-3d85696f">¥</view><view class="price-text data-v-3d85696f">{{goodsPrice}}</view></view><view class="bottom-price-block data-v-3d85696f"><block tt:if="{{isVipPrice}}"><vip-price vue-id="7326d956-3" class="data-v-3d85696f" bind:__l="__l" vue-slots="{{['default']}}">{{$root.f0}}</vip-price></block><block tt:else><view class="price-line data-v-3d85696f">{{"¥"+$root.f1}}</view></block></view><view data-event-opts="{{[['tap',[['goodsAddToCartFn',['$event']]]]]}}" class="right-bottom-add-cart-block data-v-3d85696f" catchtap="__e"><block tt:if="{{selectedNum>=1}}"><view class="goods-selected-num data-v-3d85696f"><uni-badge vue-id="7326d956-4" border="{{true}}" size="small" text="{{selectedNum}}" type="error" class="data-v-3d85696f" bind:__l="__l"></uni-badge></view></block><uni-icons vue-id="7326d956-5" color="{{configInfo.web_site_color}}" size="25" type="plus-filled" class="data-v-3d85696f" bind:__l="__l"></uni-icons></view></view>