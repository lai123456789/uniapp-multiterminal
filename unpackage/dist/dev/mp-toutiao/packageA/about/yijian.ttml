<view class="page"><block tt:for="{{list}}" tt:for-item="item" tt:for-index="index" tt:key="index"><view data-event-opts="{{[['tap',[['setActIndex',[index]]]]]}}" class="line-block" bindtap="__e"><view class="line"><text>{{item.name}}</text><view><block tt:if="{{actIndex==index}}"><uni-icons vue-id="{{'077d929e-1-'+index}}" size="25" type="circle-filled" color="{{configInfo.web_site_color}}" bind:__l="__l"></uni-icons></block><block tt:else><uni-icons vue-id="{{'077d929e-2-'+index}}" size="25" type="circle" color="#c7c7c7" bind:__l="__l"></uni-icons></block></view></view></view></block><block tt:if="{{!mobile}}"><view class="line-block"><view class="line"><text>联系人</text><input class="input" style="width:80%;" type="text" placeholder="手机号" data-event-opts="{{[['input',[['__set_model',['','tel','$event',[]]]]]]}}" value="{{tel}}" bindinput="__e"/></view></view></block><view class="line-block"><view class="line" style="border-bottom:none;"><text>意见/建议</text></view></view><view class="content-wrap"><textarea class="textarea" placeholder="请写下您的意见或建议" data-event-opts="{{[['input',[['__set_model',['','content','$event',[]]]]]]}}" value="{{content}}" bindinput="__e"></textarea></view><view class="file-list-block"><view class="content-wrap"><block tt:for="{{uploadImgList}}" tt:for-item="item" tt:for-index="index" tt:key="index"><view data-event-opts="{{[['tap',[['lookImgListFn',[index]]]]]}}" class="item border-radius20 show-image" bindtap="__e"><image class="image" src="{{item}}" mode></image><uni-icons style="position:absolute;z-index:10;top:20rpx;right:20rpx;" vue-id="{{'077d929e-3-'+index}}" size="20" type="close" color="#ffffff" data-event-opts="{{[['tap',[['delImgFn',[index]]]]]}}" catchtap="__e" bind:__l="__l"></uni-icons></view></block><view data-event-opts="{{[['tap',[['chooseImageFn',['$event']]]]]}}" class="item border-radius20 upload-btn" bindtap="__e"></view></view></view><view style="height:100rpx;"></view><view data-event-opts="{{[['tap',[['commitFn',['$event']]]]]}}" class="bottom-btn-block" bindtap="__e"><button class="{{(('pay-btn')+' '+(isAllAngle?'border-radius-style-all-arg':'border-radius-style-half-arg'))}}" style="{{'background:'+(configInfo.web_site_color)+';'}}">提交</button></view><login-fn vue-id="077d929e-4" is-show-login="{{loginBoxFlag}}" to-back="{{true}}" data-event-opts="{{[['^loginSuccessCallback',[['getFeedbackTypeFn']]],['^close',[['closeGlobalLoginFn']]]]}}" bind:loginSuccessCallback="__e" bind:close="__e" bind:__l="__l"></login-fn></view>